<div class="node immutable_node static" style="<%= node_position(node)%>" id="node_<%=node.id%>" data-id="<%= node.id %>">
  <%= form_for([pattern, node], :html => {:class => "static"}) do |f| %>
    <%= f.select(:rdf_type, options_from_collection_for_select([node.type_expression.fancy_string(true)], "to_s", "to_s"), {} ,{:class => "classSelector"}) %>
  <% end %>
</div>