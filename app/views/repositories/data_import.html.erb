<%= form_tag(graph_import_json_path(@graph), :multipart => true) do |f| %>
  <%= label_tag("JSON")%>
  <%= file_field_tag("json") %>

  <%= label_tag("Extraction Mapping")%>
  <%= select_tag("extraction_mapping", options_from_collection_for_select(ExtractionDescription.all, "id", "name")) %>

  <%= label_tag("Limit:") %>
  <%= text_field_tag("limit", "")%>
  <br />
  <%= submit_tag("Parse and Insert!", {:class => "btn"})%>
<% end %>


<%= link_to("Reset Graph!", graph_reset_path(@graph), {:class => "btn", :confirm => "Seriously?", :method => :post})%>
