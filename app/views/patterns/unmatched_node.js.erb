stopOmMode();
<% if @next_unmatched_element.nil? %>
  $.jGrowl("All elements matched!.", {theme: 'info'});
<% else %>
  <% @matched_elements.each do |match| %>
  $("#<%= div_id(match) %>").addClass("matched");
  <% end %>
  startOmMode();
  $("#<%= div_id(@next_unmatched_element) %>").click();
  $.jGrowl("Select Matching elements and Save the correspondence when ready.", {theme: 'info', sticky: true});
<% end %>