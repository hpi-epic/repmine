<table class="table table-striped table-condensed table-bordered">
  <thead>
    <tr class="info">
      <td colspan="<%= @headers.size %>">Results for '<%= @task.pattern.name%>' on <%= @task.repository.name %> (<%= @results.size%>)</td>
    </tr>
    <tr>
      <% @headers.each do |key|%>
        <th>
          <%= key %>
        </th>
      <% end %>
    </tr>
  </thead>
  <tbody>
  <% @results.each do |res_row| %>
    <tr>
      <% res_row.each do |value|%>
      <td>
        <%= value%>
      </td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>