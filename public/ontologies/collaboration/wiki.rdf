<?xml version="1.0" encoding='ISO-8859-1'?>

<!DOCTYPE rdf:RDF [
  <!ENTITY dstore "http://hpi-web.de/ns/dstore/0.1/" >
  <!ENTITY wiki   "http://hpi-web.de/ns/dstore/wiki/0.1/" >
  <!ENTITY web    "http://hpi-web.de/ns/dstore/web/0.1/" >
  <!ENTITY owl    "http://www.w3.org/2002/07/owl#" > 
  <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#" > 
  <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#" > 
  <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#" > 
  <!ENTITY dc     "http://purl.org/dc/elements/1.1/" >
  <!ENTITY foaf   "http://xmlns.com/foaf/0.1/" >  
]>

<rdf:RDF
	xmlns         ="&wiki;"
    xml:base      ="&wiki;"
    xmlns:wiki    ="&wiki;"
    xmlns:dstore  ="&dstore;"
    xmlns:web     ="&web;"    
    xmlns:owl     ="&owl;"
    xmlns:rdf     ="&rdf;"
    xmlns:rdfs    ="&rdfs;"
    xmlns:xsd     ="&xsd;"
    xmlns:dc      ="&dc;"
	  xmlns:foaf    ="&foaf;"    
>
  <!-- About the dstore Vocabulary -->
  <owl:Ontology rdf:about="&wiki;">
    <dc:title rdf:datatype="&xsd;string">d.store Wiki Ontology v0.2</dc:title>
    <dc:creator rdf:datatype="&xsd;string">Matthias Uflacker, Thomas Kowark</dc:creator>
    <dc:format rdf:datatype="&xsd;string">application/rdf+xml</dc:format>
	  <dstore:classlabel rdf:datatype="&xsd;string">wiki</dstore:classlabel>	     
  </owl:Ontology>

  <!-- =============== -->
  <!--  Classes        -->
  <!-- =============== -->
  
  <rdfs:Class rdf:about="&wiki;WikiPage">
	  <rdf:type rdf:resource="&owl;Class"/>
  	<rdfs:subClassOf rdf:resource="&dstore;OntologyResource"/>	  
  	<rdfs:subClassOf rdf:resource="&web;WebResource"/>  	
  	<rdfs:isDefinedBy rdf:resource="&wiki;"/>
  	<dstore:classlabel rdf:datatype="&xsd;string">WikiPage</dstore:classlabel>
  	<rdfs:comment rdf:datatype="&xsd;string">Wiki Pages</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="&wiki;WikiPageRevision">
	  <rdf:type rdf:resource="&owl;Class"/>
	  <rdfs:isDefinedBy rdf:resource="&wiki;"/>
	  <dstore:classlabel rdf:datatype="&xsd;string">WikiPageRevision</dstore:classlabel>
	  <rdfs:comment rdf:datatype="&xsd;string">Revision of a Wiki Page</rdfs:comment>
	  <rdfs:subClassOf rdf:resource="&wiki;WikiPage"/>
  </rdfs:Class>  
  
  <rdfs:Class rdf:about="&wiki;WikiAttachment">
  	<rdf:type rdf:resource="&owl;Class"/>
  	<rdfs:isDefinedBy rdf:resource="&wiki;"/>
  	<dstore:classlabel rdf:datatype="&xsd;string">WikiAttachment</dstore:classlabel>
  	<rdfs:comment rdf:datatype="&xsd;string">WikiPage File Attachments</rdfs:comment>
  	<rdfs:subClassOf rdf:resource="&web;WebResource"/>
  </rdfs:Class>
  
  <!-- ================== -->
  <!-- Properties         -->
  <!-- ================== -->

  <!-- Person > wikiname > string -->
  <dstore:OntologyAttribute rdf:about="&wiki;wikiname">
	<rdf:type rdf:resource="&dstore;AliasAttribute"/>
	<rdfs:label xml:lang="en">Wiki account</rdfs:label>
    <rdfs:comment xml:lang="en">A person's wiki account name</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Agent"/>
    <rdfs:range  rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyAttribute>
  
  <!-- WikiPage > createdBy > Person -->
  <dstore:OntologyRelation rdf:about="&wiki;createdBy">
	<rdfs:label xml:lang="en">created by</rdfs:label>
    <rdfs:comment xml:lang="en">The initial creator of a wiki page</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiPage"/>
    <rdfs:range  rdf:resource="&dstore;Agent"/>
    <owl:inverseOf rdf:resource="&wiki;hasCreated"/>
    <dstore:refersTo rdf:resource="&wiki;wikiname"/>
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyRelation>

  <!-- Person > hasCreated > WikiPage -->
  <dstore:OntologyRelation rdf:about="&wiki;hasCreated">
	<rdfs:label xml:lang="en">has created</rdfs:label>
    <rdfs:comment xml:lang="en">A WikiPage created by a person</rdfs:comment>
	<rdfs:domain rdf:resource="&dstore;Agent"/>	
    <rdfs:range rdf:resource="&wiki;WikiPage"/>
	<owl:inverseOf rdf:resource="&wiki;createdBy"/>
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyRelation>

  <!-- WikiPageRevision > basePage > WikiPage -->
  <dstore:OntologyRelation rdf:about="&wiki;basePage">
	<rdfs:label xml:lang="en">base page</rdfs:label>
    <rdfs:comment xml:lang="en">base Page of a wiki page revision</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiPageRevision"/>
    <rdfs:range  rdf:resource="&wiki;WikiPage"/>	
    <owl:inverseOf rdf:resource="&wiki;pageRevision"/>            
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyRelation>
  
  <dstore:OntologyRelation rdf:about="&wiki;pageRevision">
	  <rdfs:label xml:lang="en">page Revision</rdfs:label>
    <rdfs:comment xml:lang="en">revision of a wiki page</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiPage"/>
    <rdfs:range  rdf:resource="&wiki;WikiPageRevision"/>	
    <owl:inverseOf rdf:resource="&wiki;basePage"/>        
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyRelation>

  <!-- WikiPage > attachment > WikiAttachment -->
  <dstore:OntologyRelation rdf:about="&wiki;attachment">
	<rdfs:label xml:lang="en">wiki attachment</rdfs:label>
    <rdfs:comment xml:lang="en">A file attachment for a wiki page</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiPage"/>
    <rdfs:range  rdf:resource="&wiki;WikiAttachment"/>
    <owl:inverseOf rdf:resource="&wiki;attachedTo"/>	
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyRelation>
  
  <!-- WikiPage > content > string -->
  <dstore:OntologyAttribute rdf:about="&wiki;content">
  <rdfs:label xml:lang="en">content</rdfs:label>
    <rdfs:comment xml:lang="en">The content of a wikipage</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiPage"/>
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyAttribute> 
  
  <!-- WikiAttachment > attached_to > WikiPage -->
  <dstore:OntologyRelation rdf:about="&wiki;attachedTo">
	<rdfs:label xml:lang="en">attached to</rdfs:label>
    <rdfs:comment xml:lang="en">The wiki page to which this resource is attached to</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiAttachment"/>
    <rdfs:range  rdf:resource="&wiki;WikiPage"/>
    <owl:inverseOf rdf:resource="&wiki;attachment"/>    
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyRelation>
  
  <!-- WikiAttachment > content_type > string -->
  <dstore:OntologyAttribute rdf:about="&wiki;content_type">
	<rdfs:label xml:lang="en">content type</rdfs:label>
    <rdfs:comment xml:lang="en">content type of wiki attachment</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiAttachment"/>
    <rdfs:range  rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyAttribute>
  
  <!-- WikiAttachment > filename > string -->
  <dstore:OntologyAttribute rdf:about="&wiki;filename">
  <rdfs:label xml:lang="en">filename</rdfs:label>
    <rdfs:comment xml:lang="en">filename of wiki attachment</rdfs:comment>
    <rdfs:domain rdf:resource="&wiki;WikiAttachment"/>
    <rdfs:range  rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&wiki;"/>
  </dstore:OntologyAttribute>
</rdf:RDF>