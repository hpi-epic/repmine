<?xml version="1.0" encoding='ISO-8859-1'?>

<!DOCTYPE rdf:RDF [
  <!ENTITY dstore "http://hpi-web.de/ns/dstore/0.1/" >
  <!ENTITY streamworks  "http://hpi-web.de/ns/dstore/streamworks/0.1/" >
  <!ENTITY web    "http://hpi-web.de/ns/dstore/web/0.1/" >    
  <!ENTITY owl    "http://www.w3.org/2002/07/owl#" > 
  <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#" > 
  <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#" > 
  <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#" > 
  <!ENTITY dc     "http://purl.org/dc/elements/1.1/" >
  <!ENTITY foaf   "http://xmlns.com/foaf/0.1/" >
]>

<rdf:RDF
    xmlns         ="&streamworks;"
    xml:base      ="&streamworks;"
    xmlns:streamworks ="&streamworks;"
    xmlns:dstore  ="&dstore;"
    xmlns:web     ="&web;"    
    xmlns:owl     ="&owl;"
    xmlns:rdf     ="&rdf;"
    xmlns:rdfs    ="&rdfs;"
    xmlns:xsd     ="&xsd;"
    xmlns:dc      ="&dc;"
	  xmlns:foaf    ="&foaf;"
>
  <!-- About the dstore Vocabulary -->
  <owl:Ontology rdf:about="&streamworks;">
    <dc:title rdf:datatype="&xsd;string">d.store Streamworks Ontology v0.1</dc:title>
    <dc:creator rdf:datatype="&xsd;string">Thomas Kowark</dc:creator>
    <dc:format rdf:datatype="&xsd;string">application/rdf+xml</dc:format>
	  <dstore:classlabel rdf:datatype="&xsd;string">streamworks</dstore:classlabel>	        
  </owl:Ontology>

  <!-- =============== -->
  <!--  Classes        -->
  <!-- =============== -->

  <rdfs:Class rdf:about="&streamworks;StreamworksActivity">
  <rdf:type rdf:resource="&owl;Class"/>
  <rdfs:subClassOf rdf:resource="&dstore;OntologyResource"/>
  <rdfs:subClassOf rdf:resource="&web;WebResource"/>  
  <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
  <dstore:classlabel rdf:datatype="&xsd;string">Streamworks Activity</dstore:classlabel> 
  <rdfs:comment rdf:datatype="&xsd;string">Streamworks Activity</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="&streamworks;StreamworksTopic">
  <rdf:type rdf:resource="&owl;Class"/>
  <rdfs:subClassOf rdf:resource="&dstore;OntologyResource"/>
  <rdfs:subClassOf rdf:resource="&web;WebResource"/>  
  <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
  <dstore:classlabel rdf:datatype="&xsd;string">Streamworks Topic</dstore:classlabel> 
  <rdfs:comment rdf:datatype="&xsd;string">A topic in Streamworks</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="&streamworks;DiscussionThread">
  <rdf:type rdf:resource="&owl;Class"/>
  <rdfs:subClassOf rdf:resource="&dstore;OntologyResource"/>
  <rdfs:subClassOf rdf:resource="&web;WebResource"/>  
  <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
  <dstore:classlabel rdf:datatype="&xsd;string">Streamworks Discussion Thread</dstore:classlabel> 
  <rdfs:comment rdf:datatype="&xsd;string">A Discussion Thread within a Streamworks Topic</rdfs:comment>
  </rdfs:Class>
  
  <!-- ================== -->
  <!-- Properties         -->
  <!-- ================== -->

  <!-- StreamworksTopic > isTopicOf > StreamworksActivity -->
  <dstore:OntologyRelation rdf:about="&streamworks;isTopicOf"> 
    <rdfs:label xml:lang="en">isTopicOf</rdfs:label>
    <rdfs:comment xml:lang="en">The Streamworks Activity that a topic belongs to</rdfs:comment>
    <rdfs:domain rdf:resource="&streamworks;StreamworksTopic"/>
    <rdfs:range  rdf:resource="&streamworks;StreamworksActivity"/>
    <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
    <owl:inverseOf rdf:resource="&streamworks;isActivityOf"/>    
  </dstore:OntologyRelation>

  <!-- StreamworksActivity > isActivityOf > StreamworksTopic -->
  <dstore:OntologyRelation rdf:about="&streamworks;isActivityOf"> 
    <rdfs:label xml:lang="en">isActivityOf</rdfs:label>
    <rdfs:comment xml:lang="en">A Topic of a Streamworks Activity</rdfs:comment>
    <rdfs:domain rdf:resource="&streamworks;StreamworksActivity"/>
    <rdfs:range  rdf:resource="&streamworks;StreamworksTopic"/>
    <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
    <owl:inverseOf rdf:resource="&streamworks;isTopicOf"/>       
  </dstore:OntologyRelation>
  
  <!-- StreamworksTopic > discussion > DiscussionThread -->
  <dstore:OntologyRelation rdf:about="&streamworks;discussion"> 
    <rdfs:label xml:lang="en">discussion</rdfs:label>
    <rdfs:comment xml:lang="en">The Streamworks Topic that a Discussion Thread belongs to</rdfs:comment>
    <rdfs:domain rdf:resource="&streamworks;StreamworksTopic"/>
    <rdfs:range  rdf:resource="&streamworks;DiscussionThread"/>
    <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
    <owl:inverseOf rdf:resource="&streamworks;isDiscussionAbout"/>    
  </dstore:OntologyRelation>
  
  <!-- DiscussionThread > isDiscussionAbout > StreamworksTopic -->
  <dstore:OntologyRelation rdf:about="&streamworks;isDiscussionAbout"> 
    <rdfs:label xml:lang="en">isDiscussionAbout</rdfs:label>
    <rdfs:comment xml:lang="en">The Streamworks Topic that a Discussion Thread belongs to</rdfs:comment>
    <rdfs:domain rdf:resource="&streamworks;DiscussionThread"/>
    <rdfs:range  rdf:resource="&streamworks;StreamworksTopic"/>
    <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
    <owl:inverseOf rdf:resource="&streamworks;discussion"/>    
  </dstore:OntologyRelation>

  <!-- DiscussionThread > startedBy > Person -->
  <dstore:OntologyRelation rdf:about="&streamworks;startedBy">
	  <rdfs:label xml:lang="en">startedBy</rdfs:label>
    <rdfs:comment xml:lang="en">A person that started a discussion thread of a topic</rdfs:comment>
    <rdfs:domain rdf:resource="&streamworks;DiscussionThread"/>
    <rdfs:range  rdf:resource="&dstore;Agent" />
    <rdfs:isDefinedBy rdf:resource="&streamworks;" />
    <owl:inverseOf rdf:resource="&streamworks;started"/>
    <dstore:refersTo rdf:resource="&streamworks;streamworks_name"/>    
  </dstore:OntologyRelation>
  
  <!-- Person > streamworks_name > string -->
  <dstore:OntologyAttribute rdf:about="&streamworks;streamworks_name">
	  <rdf:type rdf:resource="&dstore;AliasAttribute"/>	
	  <rdfs:label xml:lang="en">streamworks name</rdfs:label>
    <rdfs:comment xml:lang="en">A person's streamworks name</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Agent"/>
    <rdfs:range  rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
  </dstore:OntologyAttribute>
  
  <!-- Person > started > StreamworksTopic -->
  <dstore:OntologyRelation rdf:about="&streamworks;started">
	  <rdfs:label xml:lang="en">started</rdfs:label>
    <rdfs:comment xml:lang="en">A Person that started a discussion thread</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Agent"/>
    <rdfs:range  rdf:resource="&streamworks;DiscussionThread"/>
    <owl:inverseOf rdf:resource="&streamworks;startedBy"/>
    <rdfs:isDefinedBy rdf:resource="&streamworks;"/>
  </dstore:OntologyRelation>  
</rdf:RDF>