<?xml version="1.0" encoding='ISO-8859-1'?>

<!DOCTYPE rdf:RDF [
  <!ENTITY dstore "http://hpi-web.de/ns/dstore/0.1/" >
  <!ENTITY owl    "http://www.w3.org/2002/07/owl#" > 
  <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#" > 
  <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#" > 
  <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#" > 
  <!ENTITY dc     "http://purl.org/dc/elements/1.1/" >
  <!ENTITY foaf   "http://xmlns.com/foaf/0.1/" >
  <!ENTITY doap   "http://usefulinc.com/ns/doap#" >
]>

<rdf:RDF
	  xmlns         ="&dstore;"
    xml:base      ="&dstore;"
    xmlns:dstore  ="&dstore;"
    xmlns:owl     ="&owl;"
    xmlns:rdf     ="&rdf;"
    xmlns:rdfs    ="&rdfs;"
    xmlns:xsd     ="&xsd;"
    xmlns:dc      ="&dc;"
	  xmlns:foaf    ="&foaf;"
	  xmlns:doap	  ="&doap;"
>
  <!-- About the dstore Vocabulary -->
  <owl:Ontology rdf:about="&dstore;">
    <dc:title rdf:datatype="&xsd;string">d.store Ontology v0.7</dc:title>
    <dc:format rdf:datatype="&xsd;string">application/rdf+xml</dc:format>
	  <dstore:classlabel rdf:datatype="&xsd;string">dstore</dstore:classlabel>	             
  </owl:Ontology>
  
  <owl:AnnotationProperty rdf:about="&dc;title"/>
  <owl:AnnotationProperty rdf:about="&dc;creator"/>
  <owl:AnnotationProperty rdf:about="&dc;format"/>
  <owl:Class rdf:about="&rdfs;Class"/>  

  <!-- =============== -->
  <!-- d.store Classes -->
  <!-- =============== -->

  <owl:Class rdf:about="&dstore;Model">
	<rdfs:subClassOf rdf:resource="&owl;Ontology" />
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:Class>

  <owl:Class rdf:about="&dstore;GraphConceptModel">
	<rdfs:subClassOf rdf:resource="&dstore;Model" />
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:Class>

  <owl:Class rdf:about="&dstore;GraphInstanceModel">
	<rdfs:subClassOf rdf:resource="&dstore;Model" />
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:Class>

  <owl:Class rdf:about="&dstore;GraphImportModel">
	<rdfs:subClassOf rdf:resource="&dstore;Model" />
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:Class>
  
  <owl:Class rdf:about="&dstore;Resource">
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
	<rdfs:label rdf:datatype="&xsd;string">Resource</rdfs:label>
	<rdfs:comment rdf:datatype="&xsd;string">A resource representing not further classified information in a Graph</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="&dstore;OntologyResource">
	<rdfs:subClassOf rdf:resource="&dstore;Resource" />		
	<rdfs:label rdf:datatype="&xsd;string">A global resource type defined by d.store</rdfs:label>
	<rdfs:comment rdf:datatype="&xsd;string">...</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>	
  </owl:Class>

  <owl:Class rdf:about="&dstore;GraphResource">
	<rdfs:subClassOf rdf:resource="&dstore;Resource"/>
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
	<rdfs:label rdf:datatype="&xsd;string">A resource type defined on graph-level.</rdfs:label>
	<rdfs:comment rdf:datatype="&xsd;string">Abstract unclassified resource type defined by a graph ontology. Superclass for all graph-defined resource classes.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="&dstore;UserResource">
	<rdfs:subClassOf rdf:resource="&dstore;Resource"/>  
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
	<rdfs:label rdf:datatype="&xsd;string">Custom resource type defined by users.</rdfs:label>
	<rdfs:comment rdf:datatype="&xsd;string">Abstract unclassified resource type defined by a project community. Superclass for all community-defined resource classes.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="&dstore;Graph">
	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
	<rdfs:label rdf:datatype="&xsd;string">Team Communication Network</rdfs:label>
	<rdfs:comment rdf:datatype="&xsd;string">A graph representing a Team Communication Network</rdfs:comment>
	<rdfs:subClassOf rdf:resource="&doap;Project" />
	<owl:disjointWith rdf:resource="&dstore;Role"/>
	<owl:disjointWith rdf:resource="&dstore;Person"/>
  </owl:Class>

  <owl:Class rdf:about="&dstore;Person">
  	<rdfs:subClassOf rdf:resource="&foaf;Person" />
    <rdfs:subClassOf rdf:resource="&dstore;Agent" />  	
  	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  	<dstore:classlabel rdf:datatype="&xsd;string">Person</dstore:classlabel>
  	<rdfs:comment rdf:datatype="&xsd;string">People</rdfs:comment>
  	<owl:disjointWith rdf:resource="&dstore;Role"/>
  	<owl:disjointWith rdf:resource="&dstore;Graph"/>
  </owl:Class>
  
  <owl:Class rdf:about="&dstore;Team">
    <rdfs:subClassOf rdf:resource="&foaf;Group" />
    <rdfs:subClassOf rdf:resource="&dstore;Agent" />    
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
    <dstore:classlabel rdf:datatype="&xsd;string">Team</dstore:classlabel>
  	<rdfs:comment rdf:datatype="&xsd;string">Team</rdfs:comment>
  	<owl:disjointWith rdf:resource="&dstore;Person"/>
  </owl:Class>
  
  <owl:Class rdf:about="&dstore;Agent">
    <rdfs:subClassOf rdf:resource="&foaf;Agent" />
  	<rdfs:subClassOf rdf:resource="&dstore;OntologyResource" />	    
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>  	
    <dstore:classlabel rdf:datatype="&xsd;string">Agent</dstore:classlabel>
  	<rdfs:comment rdf:datatype="&xsd;string">Agent</rdfs:comment>    
  </owl:Class>
  
  <owl:Class rdf:about="&dstore;User">
  	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  	<rdfs:label rdf:datatype="&xsd;string">A d.store user</rdfs:label>
  	<rdfs:subClassOf rdf:resource="&dstore;Agent" />
  </owl:Class>
  
  <owl:Class rdf:about="&dstore;Role">
  	<rdfs:isDefinedBy rdf:resource="&dstore;"/>
  	<rdfs:label rdf:datatype="&xsd;string">A user's role</rdfs:label>
  	<rdfs:comment rdf:datatype="&xsd;string">A Role defines a set of privileges and rights</rdfs:comment>
  	<owl:disjointWith rdf:resource="&dstore;Graph"/>
    <owl:disjointWith rdf:resource="&dstore;User"/>
  </owl:Class>

  <owl:Class rdf:about="&dstore;Attribute">
    <rdfs:subClassOf rdf:resource="&owl;DatatypeProperty" />
    <owl:disjointWith rdf:resource="&dstore;Relation"/>    
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <owl:Class rdf:about="&dstore;OntologyAttribute">
    <rdfs:subClassOf rdf:resource="&dstore;Attribute" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  	<owl:disjointWith rdf:resource="&dstore;GraphAttribute"/>
  	<owl:disjointWith rdf:resource="&dstore;UserAttribute"/>
  </owl:Class>

  <owl:Class rdf:about="&dstore;GraphAttribute">
    <rdfs:subClassOf rdf:resource="&dstore;Attribute" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  	<owl:disjointWith rdf:resource="&dstore;OntologyAttribute"/>
  	<owl:disjointWith rdf:resource="&dstore;UserAttribute"/>	
  </owl:Class>

  <owl:Class rdf:about="&dstore;UserAttribute">
    <rdfs:subClassOf rdf:resource="&dstore;Attribute" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
	<owl:disjointWith rdf:resource="&dstore;OntologyAttribute"/>	
	<owl:disjointWith rdf:resource="&dstore;GraphAttribute"/>	
  </owl:Class>

  <owl:Class rdf:about="&dstore;AliasAttribute">
    <rdfs:subClassOf rdf:resource="&dstore;Attribute" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <owl:Class rdf:about="&dstore;AliasReferenceAttribute">
    <rdfs:subClassOf rdf:resource="&dstore;Attribute" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <owl:Class rdf:about="&dstore;Relation">
    <rdfs:subClassOf rdf:resource="&owl;ObjectProperty" />
    <owl:disjointWith rdf:resource="&dstore;Attribute" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <owl:Class rdf:about="&dstore;OntologyRelation">
    <rdfs:subClassOf rdf:resource="&dstore;Relation" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <owl:Class rdf:about="&dstore;GraphRelation">
    <rdfs:subClassOf rdf:resource="&dstore;Relation" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <owl:Class rdf:about="&dstore;UserRelation">
    <rdfs:subClassOf rdf:resource="&dstore;Relation" />
    <rdfs:isDefinedBy rdf:resource="&dstore;" />
  </owl:Class>

  <!-- ================== -->
  <!-- d.store Properties -->
  <!-- ================== -->

  <!-- OntologyRelation > refersTo > OntologyAttribute -->
  <rdf:Property rdf:about="&dstore;refersTo">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:label xml:lang="en">refersTo</rdfs:label>
    <rdfs:comment xml:lang="en">an ontologyRelation refers to an Ontology Attribute</rdfs:comment>
    <rdfs:domain rdf:resource="&owl;OntologyRelation"/>
    <rdfs:range  rdf:resource="&owl;OntologyAttribute"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>  

  <!-- Role > isAssignedTo > User -->
  <rdf:Property rdf:about="&dstore;isAssignedTo">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:label xml:lang="en">is assigned to</rdfs:label>
    <rdfs:comment xml:lang="en">a person that has this role</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Role"/>
    <rdfs:range  rdf:resource="&dstore;User"/>
    <owl:inverseOf rdf:resource="&dstore;hasRole"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>

  <!-- User > hasRole > Role -->
  <rdf:Property rdf:about="&dstore;hasRole">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:label xml:lang="en">has Role</rdfs:label>
    <rdfs:comment xml:lang="en">assigns a Role to a Person</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;User"/>
    <rdfs:range  rdf:resource="&dstore;Role"/>
    <owl:inverseOf rdf:resource="&dstore;isAssignedTo"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>

  <!-- User > memberOf > Graph -->
  <owl:ObjectProperty rdf:about="&dstore;memberOf">
    <rdfs:domain rdf:resource="&dstore;User"/>
    <rdfs:range rdf:resource="&dstore;Graph"/>
    <owl:inverseOf rdf:resource="&dstore;member"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- Graph > member > User -->
  <owl:ObjectProperty rdf:about="&dstore;member">
    <rdfs:domain rdf:resource="&dstore;Graph"/>
    <rdfs:range  rdf:resource="&dstore;User"/>
    <owl:inverseOf rdf:resource="&dstore;memberOf"/>    
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- User > ownerOf > Graph -->
  <owl:ObjectProperty rdf:about="&dstore;ownerOf">
    <rdfs:domain rdf:resource="&dstore;User"/>
    <rdfs:range  rdf:resource="&dstore;Graph"/>
    <rdfs:subPropertyOf rdf:resource="&dstore;memberOf" />
    <owl:inverseOf rdf:resource="&dstore;owner"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- Graph > owner > User -->
  <owl:ObjectProperty rdf:about="&dstore;owner">
    <rdfs:domain rdf:resource="&dstore;Graph"/>
    <rdfs:range  rdf:resource="&dstore;User"/>
    <rdfs:subPropertyOf rdf:resource="&dstore;member" />
    <owl:inverseOf rdf:resource="&dstore;ownerOf"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>
  
  <!-- Graph > conceptModel > GraphConceptModel -->
  <owl:ObjectProperty rdf:about="&dstore;conceptModel">
    <rdfs:domain rdf:resource="&dstore;Graph"/>
    <rdfs:range  rdf:resource="&dstore;GraphConceptModel"/>
    <owl:inverseOf rdf:resource="&dstore;project"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- GraphConceptModel > project > Graph -->
  <owl:ObjectProperty rdf:about="&dstore;project">
    <rdfs:domain rdf:resource="&dstore;GraphConceptModel"/>
    <rdfs:range  rdf:resource="&dstore;Graph"/>
    <owl:inverseOf rdf:resource="&dstore;conceptModel"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- Graph > instanceModel > GraphInstanceModel -->
  <owl:ObjectProperty rdf:about="&dstore;instanceModel">
    <rdfs:domain rdf:resource="&dstore;Graph"/>
    <rdfs:range  rdf:resource="&dstore;GraphInstanceModel"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- Graph > importModel > GraphImportModel -->
  <owl:ObjectProperty rdf:about="&dstore;importModel">
    <rdfs:domain rdf:resource="&dstore;Graph"/>
    <rdfs:range  rdf:resource="&dstore;GraphImportModel"/>	
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>  

  <!-- Resource > classlabel > xsd:string -->
  <rdf:Property rdf:about="&dstore;classlabel">
    <rdfs:domain rdf:resource="&rdfs;Class"/>
    <rdfs:range  rdf:resource="&rdfs;Literal"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>
  
  <!-- Attribute > aliasAttribute > Attribute -->
  <rdf:Property rdf:about="&dstore;aliasAttribute">
    <rdfs:domain rdf:resource="&dstore;AliasReferenceAttribute"/>
    <rdfs:range  rdf:resource="&dstore;AliasAttribute"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>  
  
  <!-- Ontology > ns_label > xsd:string -->
  <rdf:Property rdf:about="&dstore;ns_label">
    <rdf:type rdf:resource="&owl;DatatypeProperty"/>
    <rdfs:domain rdf:resource="&owl;Ontology"/>  
    <rdfs:range  rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>  

  <!-- Resource > instance_id > positiveInteger -->	
  <owl:DatatypeProperty rdf:about="&dstore;instance_id">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
	  <rdfs:label xml:lang="en">Instance Index</rdfs:label>
    <rdfs:comment xml:lang="en">Resource Index</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Resource"/>
    <rdfs:range  rdf:resource="&xsd;positiveInteger"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:DatatypeProperty>

  <!-- Graph > resource_index > nonNegativeInteger -->	 
  <owl:DatatypeProperty rdf:about="&dstore;resource_index">
	<rdfs:label xml:lang="en">resourceIndex</rdfs:label>
    <rdfs:comment xml:lang="en">A counter for the unique resource indices.</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Graph"/>
    <rdfs:range  rdf:resource="&xsd;nonNegativeInteger"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:DatatypeProperty>

  <!-- User > password > string -->	 
  <rdf:Property rdf:about="&dstore;password">
    <rdf:type rdf:resource="&owl;DatatypeProperty"/>
	<rdfs:label xml:lang="en">password</rdfs:label>
    <rdfs:domain rdf:resource="&dstore;User"/>
    <rdfs:range  rdf:resource="&xsd;string"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </rdf:Property>
  
  <!-- Now the explicit date representation ... -->
  <!-- Resource > created_at > xsd:dateTime -->	 
  <owl:DatatypeProperty rdf:about="&dstore;created_at">
	  <rdfs:label xml:lang="en">created_at</rdfs:label>
    <rdfs:comment xml:lang="en">Defines when a Resource was created</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;OntologyResource"/>
    <rdfs:range  rdf:resource="&xsd;dateTime"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:DatatypeProperty>

  <!-- Resource > deleted_at > xsd:dateTime -->	 
  <owl:DatatypeProperty rdf:about="&dstore;deleted_at">
	  <rdfs:label xml:lang="en">deleted_at</rdfs:label>
    <rdfs:comment xml:lang="en">Defines when a Resource stopped existing.</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Resource"/>
    <rdfs:range  rdf:resource="&xsd;dateTime"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:DatatypeProperty>

  <!-- Resource > createdBy > User -->	 
  <owl:ObjectProperty rdf:about="&dstore;createdBy">
	<rdfs:label xml:lang="en">createdBy</rdfs:label>
    <rdfs:comment xml:lang="en">Defines who uploaded a Resource to the dstore.</rdfs:comment>
    <rdfs:domain rdf:resource="&dstore;Resource"/>
    <rdfs:range  rdf:resource="&dstore;User"/>
    <rdfs:isDefinedBy rdf:resource="&dstore;"/>
  </owl:ObjectProperty>

  <!-- Predefined Individiuals -->
  <dstore:Role rdf:about="&dstore;Everyone">
  </dstore:Role>

  <dstore:Role rdf:about="&dstore;Nobody">
  </dstore:Role>
  
</rdf:RDF>